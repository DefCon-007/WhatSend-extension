{
  "name": "What-Send",
  "version": "1.0",
  "description": "A utility extension to send message from whatsapp web automatically.",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "all_frames": true,
      "js": ["content_script.js"]
    }
  ],
  "permissions": ["tabs", "storage", "https://web.whatsapp.com/*"]
}
