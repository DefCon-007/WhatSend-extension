 {
   "name": "Whats-Send",
   "version": "1.0",
   "description": "My first Chrome extension.",
   "manifest_version": 2,
   "browser_action": {
     "default_popup": "popup.html"
   },
   "content_scripts": [{
    "matches": ["<all_urls>"],
    "all_frames": true,
    "js": ["content_script.js"]
    }],
   "permissions": [
    "tabs",
    "storage",
    "https://web.whatsapp.com/*"
  ]
 }